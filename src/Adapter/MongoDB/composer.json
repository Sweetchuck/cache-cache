{
    "name": "cache/mongodb-adapter",
    "description": "A PSR-6 cache implementation using MongoDB. This implementation supports tags",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "cache",
        "psr-6",
        "mongodb",
        "tag"
    ],
    "authors": [
        {
            "name": "Aaron Scherer",
            "email": "aequasi@gmail.com",
            "homepage": "https://github.com/aequasi"
        },
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com",
            "homepage": "https://github.com/nyholm"
        }
    ],
    "homepage": "https://www.php-cache.com/en/latest/",
    "require": {
        "php": ">=8.1",
        "cache/adapter-common": "^1.1",
        "mongodb/mongodb": "^1.0",
        "psr/cache": "^3.0",
        "psr/simple-cache": "^3.0"
    },
    "require-dev": {
        "cache/integration-tests": "^3.x-dev",
        "phpunit/phpunit": "^10.0"
    },
    "provide": {
        "psr/cache-implementation": "^3.0",
        "psr/simple-cache-implementation": "^3.0"
    },
    "repositories": {
        "cache/integration-tests": {
            "type": "git",
            "url": "https://github.com/Sweetchuck/cache-integration-tests.git"
        },
        "tedivm/stash": {
            "type": "github",
            "url": "https://github.com/Sweetchuck/tedivm-stash.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cache\\Adapter\\MongoDB\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
