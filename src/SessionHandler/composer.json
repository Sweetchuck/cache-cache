{
    "name": "cache/session-handler",
    "description": "An implementation of PHP's SessionHandlerInterface that allows PSR-6",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "cache",
        "psr-6",
        "session handler"
    ],
    "authors": [
        {
            "name": "Aaron Scherer",
            "email": "aequasi@gmail.com",
            "homepage": "https://github.com/aequasi"
        },
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com",
            "homepage": "https://github.com/nyholm"
        },
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "https://github.com/prisis"
        }
    ],
    "homepage": "https://www.php-cache.com/en/latest/",
    "require": {
        "php": ">=8.1",
        "psr/cache": "^3.0"
    },
    "require-dev": {
        "cache/array-adapter": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "repositories": {
        "cache/integration-tests": {
            "type": "git",
            "url": "https://github.com/Sweetchuck/cache-integration-tests.git"
        },
        "tedivm/stash": {
            "type": "github",
            "url": "https://github.com/Sweetchuck/tedivm-stash.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cache\\SessionHandler\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
