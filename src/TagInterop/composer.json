{
    "name": "cache/tag-interop",
    "description": "Framework interoperable interfaces for tags",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "cache",
        "psr6",
        "tag",
        "psr"
    ],
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com",
            "homepage": "https://github.com/nyholm"
        },
        {
            "name": "Nicolas Grekas ",
            "email": "p@tchwork.com",
            "homepage": "https://github.com/nicolas-grekas"
        }
    ],
    "homepage": "https://www.php-cache.com/en/latest/",
    "require": {
        "php": ">=8.1",
        "psr/cache": "^3.0"
    },
    "repositories": {
        "cache/integration-tests": {
            "type": "git",
            "url": "https://github.com/Sweetchuck/cache-integration-tests.git"
        },
        "tedivm/stash": {
            "type": "github",
            "url": "https://github.com/Sweetchuck/tedivm-stash.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cache\\TagInterop\\": ""
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
