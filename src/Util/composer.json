{
    "name": "cache/util",
    "description": "PSR-6 and PSR-16 Caching Utilities",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "cache",
        "psr6",
        "psr16",
        "utilities",
        "util"
    ],
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com",
            "homepage": "https://github.com/nyholm"
        },
        {
            "name": "RJ Garcia",
            "email": "rj@bighead.net",
            "homepage": "https://github.com/ragboyjr"
        }
    ],
    "homepage": "https://www.php-cache.com/en/latest/",
    "require": {
        "php": ">=8.1",
        "psr/cache": "^3.0"
    },
    "require-dev": {
        "cache/array-adapter": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "repositories": {
        "cache/integration-tests": {
            "type": "git",
            "url": "https://github.com/Sweetchuck/cache-integration-tests.git"
        },
        "tedivm/stash": {
            "type": "github",
            "url": "https://github.com/Sweetchuck/tedivm-stash.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cache\\Util\\": ""
        },
        "files": [
            "SimpleCache/util.php"
        ],
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
